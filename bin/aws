echo "Add aws/tab to brew"
brew tap aws/tap

echo "Install awscli..."
brew install awscli

echo "Install aws sam cli..."
brew install aws-sam-cli

echo "Install gimme-aws-creds"
brew install gimme-aws-creds

echo "Install k8s cli"
brew install kubernetes-cli

echo "Install k9s"
brew install derailed/k9s/k9s
