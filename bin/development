echo "Installing xcpde-select..."
xcode-select --install

echo "Installing Git..."
brew install git --override-system-git

echo "Installing MacVim..."
brew cask install macvim --override-system-vim

echo "Installing Google Chrome..."
brew cask install google-chrome

echo "Installing PHPStorm with Java6"
brew cask install caskroom/homebrew-versions/java6
brew cask install phpstorm

echo "Installing Golang"
brew cask install golang

echo "Create go working dir"
mkdir -p $HOME/Work/go

echo "Installing docker"
brew install docker docker-machine docker-machine-nfs

echo "Installing VirtualBox"
brew cask install virtualbox

echo "Done."
